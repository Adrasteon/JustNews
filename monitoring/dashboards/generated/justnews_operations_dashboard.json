{
  "id": null,
  "uid": "ef37elu2756o0e",
  "title": "JustNews Operations Dashboard",
  "description": "Content pipeline operations view covering scheduler, crawler, and Stage B throughput",
  "tags": [
    "operations",
    "pipeline",
    "stage-b"
  ],
  "timezone": "UTC",
  "refresh": "2m",
  "time": {
    "from": "now-12h",
    "to": "now"
  },
  "timepicker": {},
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Domains Crawled (6h)",
      "type": "stat",
      "description": "Domains processed by the scheduler over the last 6h",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(increase(justnews_crawler_scheduler_domains_crawled_total[6h]))",
          "legendFormat": "Domains Crawled (6h)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 2,
      "title": "Articles Accepted (6h)",
      "type": "stat",
      "description": "Accepted articles emitted by the scheduler over the last 6h",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(increase(justnews_crawler_scheduler_articles_accepted_total[6h]))",
          "legendFormat": "Articles Accepted (6h)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 3,
      "title": "Adaptive Articles (6h)",
      "type": "stat",
      "description": "Adaptive Crawl4AI output volume (6h)",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(increase(justnews_crawler_scheduler_adaptive_articles_total[6h]))",
          "legendFormat": "Adaptive Articles (6h)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 4,
      "title": "Editorial Acceptance (24h)",
      "type": "stat",
      "description": "Acceptance ratio delivered by the editorial harness",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(increase(justnews_stage_b_editorial_harness_total{result=\"accepted\"}[24h])) / clamp_min(sum(increase(justnews_stage_b_editorial_harness_total[24h])), 1)",
          "legendFormat": "Editorial Acceptance (24h)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "decimals": 2
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 5,
      "title": "Scheduler Lag (p95)",
      "type": "timeseries",
      "description": "P95 lag between scheduled and actual crawl start times",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "quantile_over_time(0.95, justnews_crawler_scheduler_lag_seconds[5m])",
          "legendFormat": "lag p95"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "id": 6,
      "title": "Crawler Throughput (req/s)",
      "type": "timeseries",
      "description": "Rolling request throughput for crawler agents",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "expr": "sum(rate(justnews_requests_total{agent=\"crawler\"}[5m]))",
          "legendFormat": "crawler req/s"
